{
	"compilerOptions": {
		"allowJs": true,
    "esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"isolatedModules": true,
		"moduleResolution": "node",
		"outDir": "./built",
    "resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "es5"
	},
	"include": ["./src/**/*"]
}
